{
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx",
    "ruby",
    "lua",
    "python",
    "css",
    "markdown",
    "Dockerfile"
